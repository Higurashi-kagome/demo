<?xml version="1.0" encoding="utf-8"?>
<litepal>
    <!--数据库名称-->
    <dbname value="holder"/>

    <!--数据库版本号。每次您要更新库时，使其值加一-->
    <version value="3" />

    <!--映射类-->
    <list>
        <mapping class="com.chalco.holder.entity.User"></mapping>
        <mapping class="com.chalco.holder.entity.Permission"></mapping>
    </list>

    <!--数据库文件的存储位置。 值为“internal” 或 “external”-->
    <storage value="internal" />

</litepal>